{
  "manifest_version": 2,
  "name": "BastardBowl",
  "description": "Replaces 'Battle of Bastards' with 'BastardBowl'.",
  "version": "0.1",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts":
  [
    {
      "matches": ["<all_urls>"],
      "js": ["bastardbowl.js"],
      "run_at": "document_end"
    }
  ]
}
